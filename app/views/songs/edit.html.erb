<h1>Edit a Song</h1>
<%= form_for @song do |s| %>
    <%= s.label :name %>
    <br>
    <%= s.text_field :name %>
    <br>

    <%= s.label :artist %>
    <br>
    <%= s.text_field :artist_id %>
    <br>

    <%= s.label :genre %>
    <br>
    <%= s.text_field :genre_id %>
    <br>

    <%= s.submit "Update Song" %>
<% end %>